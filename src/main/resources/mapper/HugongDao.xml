<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dao.HugongDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.entity.HugongEntity" id="hugongMap">
        <result property="hugonggonghao" column="hugonggonghao"/>
        <result property="mima" column="mima"/>
        <result property="hugongxingming" column="hugongxingming"/>
        <result property="xingbie" column="xingbie"/>
        <result property="hugongzhaopian" column="hugongzhaopian"/>
        <result property="nianling" column="nianling"/>
        <result property="hugongdianhua" column="hugongdianhua"/>
        <result property="minzu" column="minzu"/>
        <result property="xueli" column="xueli"/>
        <result property="hunfou" column="hunfou"/>
        <result property="hulijibie" column="hulijibie"/>
    </resultMap>

	<select id="selectListVO"
		resultType="com.entity.vo.HugongVO" >
		SELECT * FROM hugong  hugong         
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectVO"
		resultType="com.entity.vo.HugongVO" >
		SELECT  hugong.* FROM hugong  hugong
 		<where> 1=1 ${ew.sqlSegment}</where>
	</select>

    <select id="selectListView"
		resultType="com.entity.view.HugongView" >

		SELECT  hugong.* FROM hugong  hugong 	        
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.entity.view.HugongView" >
		SELECT * FROM hugong  hugong <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	

	
</mapper>
